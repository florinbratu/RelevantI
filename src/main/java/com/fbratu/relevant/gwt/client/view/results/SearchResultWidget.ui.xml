<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:gwt='urn:import:com.google.gwt.user.client.ui'>
    <ui:style src="SearchResultsWidgetStatic.css"/>
    <gwt:HTMLPanel addStyleNames="{style.result}">
        <gwt:VerticalPanel>
           <gwt:Anchor ui:field="resultTitle"
                       addStyleNames="{style.resultLink}"></gwt:Anchor>
           <gwt:Label ui:field="resultDescription"
                   addStyleNames="{style.resultDescrption}"></gwt:Label>
        </gwt:VerticalPanel>
    </gwt:HTMLPanel>
</ui:UiBinder>